<p id="notice"><%= notice %></p>

<p>
  <b>Name:</b>
  <%= @project.name %>
</p>

<% unless @project.complements.empty?  %>
  <p>
    <b>Complements:</b>
    <% @project.complements.each do |complement| %>
      <% unless complement.name.empty? %>
        <div style="margin-left: 50px"> * <%= complement.name %></div>
      <% end %>
    <% end %>
  </p>
<% end %>


<%= link_to 'Edit', edit_project_path(@project) %> |
<%= link_to 'Back', projects_path %>
